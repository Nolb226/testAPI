{
    "version": 2,
    "builds": [
      { "src": "package.json", "use": "@vercel/node" },
      { "src": "build/", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.js" }
    ],
    "env": {
      "MY_ENV_VAR": "my-value"
    },
    "build": {
      "env": {
        "BUILD_ENV_VAR": "build-value"
      },
      "command": "npm run build"
    }
  }